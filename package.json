{
  "name": "@edgeworkscreative/ewc-slides",
  "version": "0.0.7",
  "description": "Lightweight slider component built with StencilJS",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch",
    "changelog": "conventional-changelog -i CHANGELOG.md -s"
  },
  "devDependencies": {
    "@stencil/core": "^0.12.0-8",
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2",
    "tslint": "^5.11.0",
    "tslint-ionic-rules": "0.0.17",
    "conventional-changelog-cli": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edgeworkscreative/EWC-Slides.git"
  },
  "author": "Edgeworks Creative",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edgeworkscreative/EWC-Slides"
  },
  "homepage": "https://github.com/edgeworkscreative/EWC-Slides",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
